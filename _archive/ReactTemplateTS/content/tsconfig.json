{
  "compilerOptions": {
    "outDir": ".wwwroot/dist/",
    "noImplicitAny": false,
    "strict": false,
    "sourceMap": true,
    "lib": ["es6", "dom"],
    "module": "esnext",
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node"
  },
  "exclude": ["wwwroot", "node_modules"]
}
